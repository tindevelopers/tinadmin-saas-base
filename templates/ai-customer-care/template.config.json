{
  "name": "TinAdmin AI Customer Care",
  "description": "Enterprise-grade admin platform for managing AI voice agents, chat conversations, call analytics, and omnichannel customer interactions",
  "version": "1.0.0",
  "features": [
    "real_time_dashboard",
    "voice_agent_management",
    "chat_agent_management",
    "call_analytics",
    "live_call_monitoring",
    "conversation_transcripts",
    "agent_performance_metrics",
    "call_recording_library",
    "llm_configuration",
    "prompt_engineering",
    "knowledge_base_sync",
    "telephony_integration",
    "webhook_management",
    "api_integration_hub",
    "call_flow_builder",
    "ivr_navigation_setup",
    "warm_transfer_settings",
    "sentiment_analysis",
    "quality_monitoring",
    "compliance_reporting",
    "user_role_management",
    "multilingual_support",
    "voicemail_detection",
    "spam_call_filtering",
    "branded_caller_id",
    "batch_calling",
    "crm_integration",
    "calendar_scheduling",
    "notification_center",
    "audit_logs"
  ],
  "theme": {
    "colors": {
      "primary": "#4F46E5",
      "secondary": "#06B6D4",
      "success": "#10B981",
      "warning": "#F59E0B",
      "danger": "#EF4444",
      "info": "#3B82F6",
      "dark": "#1F2937",
      "light": "#F9FAFB",
      "accent": "#8B5CF6"
    },
    "typography": {
      "font_family": "Inter, system-ui, sans-serif",
      "heading_font": "Poppins, sans-serif"
    },
    "layout": {
      "sidebar_width": "280px",
      "header_height": "64px",
      "border_radius": "8px",
      "spacing_unit": "8px"
    }
  },
  "real_time_features": {
    "websocket_enabled": true,
    "auto_refresh_interval": "5000ms",
    "live_notifications": true,
    "presence_indicators": true,
    "collaborative_editing": false
  },
  "data_visualization": {
    "chart_library": "recharts",
    "default_date_range": "last_7_days",
    "export_capabilities": true,
    "custom_dashboard_builder": true
  },
  "mobile_responsive": true,
  "accessibility": {
    "wcag_compliance": "AA",
    "keyboard_navigation": true,
    "screen_reader_support": true,
    "high_contrast_mode": true
  },
  "performance": {
    "lazy_loading": true,
    "caching_strategy": "aggressive",
    "cdn_enabled": true,
    "code_splitting": true
  },
  "security": {
    "authentication": "jwt_oauth2",
    "encryption": "AES-256",
    "compliance": ["SOC2", "GDPR", "HIPAA", "PCI-DSS"],
    "audit_logging": true,
    "data_masking": true
  },
  "dependencies": {}
}
